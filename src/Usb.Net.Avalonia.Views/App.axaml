<Application 
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    x:Class="Usb.Net.Avalonia.Views.App"
    RequestedThemeVariant="Default">
    <Application.Styles>
        <FluentTheme />
		<StyleInclude Source="Styles/HeaderedContentControl.axaml" />
    </Application.Styles>

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceInclude
                    Source='/Resources/Converters.axaml' />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>